export EDITOR=/Applications/Emacs.app/Contents/MacOS/bin/emacsclient

PATH="$HOME/tools/local/bin:$PATH:$HOME/code/security/bin"
export PATH

export PYENV_ROOT="$HOME/.pyenv"

alias status='git status'
